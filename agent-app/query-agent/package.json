{
  "name": "imodel-query-agent",
  "version": "0.0.0",
  "private": true,
  "description": "Demonstrates using iModel.js to create an agent that queries changes to an iModel",
  "license": "MIT",
  "scripts": {
    "build": "tsc 1>&2",
    "clean": "rimraf lib tsconfig.tsbuildinfo .rush",
    "lint": "tslint --project . 1>&2",
    "test": "betools test --grep \"#integration\" --invert",
    "test:integration": "betools test --timeout 240000 --grep \"#integration\"",
    "cover": "nyc npm test",
    "start:debug": "node --inspect=12345 ./lib/Main.js",
    "start": "node ./lib/Main.js"
  },
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "dependencies": {
    "@bentley/bentleyjs-core": "^2.4.0-dev.17",
    "@bentley/context-registry-client": "^2.4.0-dev.17",
    "@bentley/geometry-core": "^2.4.0-dev.17",
    "@bentley/imodelhub-client": "^2.4.0-dev.17",
    "@bentley/imodeljs-backend": "^2.4.0-dev.17",
    "@bentley/backend-itwin-client": "^2.4.0-dev.17",
    "@bentley/imodeljs-common": "^2.4.0-dev.17",
    "@bentley/itwin-client": "^2.4.0-dev.17",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@bentley/build-tools": "^2.4.0-dev.17",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.7",
    "chai": "^4.2.0",
    "debug": "^3.1.0",
    "imodel-changeset-test-utility": "0.0.57",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.0.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.16.0",
    "typemoq": "^2.1.0",
    "typescript": "~3.7.4"
  },
  "nyc": {
    "nycrc-path": "./node_modules/@bentley/build-tools/.nycrc"
  }
}