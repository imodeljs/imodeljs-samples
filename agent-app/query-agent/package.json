{
  "name": "imodel-query-agent",
  "version": "0.0.0",
  "private": true,
  "description": "Demonstrates using iModel.js to create an agent that queries changes to an iModel",
  "license": "MIT",
  "scripts": {
    "build": "tsc 1>&2",
    "clean": "rimraf lib tsconfig.tsbuildinfo .rush",
    "lint": "tslint --project . 1>&2",
    "test": "node ./node_modules/@bentley/build-tools/scripts/test.js --grep \"#integration\" --invert",
    "test:integration": "node ./node_modules/@bentley/build-tools/scripts/test.js --timeout 240000 --grep \"#integration\"",
    "cover": "nyc npm test",
    "start:debug": "node --inspect=12345 ./lib/Main.js",
    "start": "node ./lib/Main.js"
  },
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "dependencies": {
    "@bentley/bentleyjs-core": "1.13.0",
    "@bentley/geometry-core": "1.13.0",
    "@bentley/imodeljs-backend": "1.13.0",
    "@bentley/imodeljs-clients": "1.13.0",
    "@bentley/imodeljs-clients-backend": "1.13.0",
    "@bentley/imodeljs-common": "1.13.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "minimist": "^1.2.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@bentley/build-tools": "1.13.0",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.0",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.7",
    "@types/supertest": "^2.0.5",
    "chai": "^4.2.0",
    "debug": "^3.1.0",
    "imodel-changeset-test-utility": "0.0.57",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.0.0",
    "rimraf": "^2.6.2",
    "supertest": "^4.0.2",
    "tslint": "^5.16.0",
    "typemoq": "^2.1.0",
    "typescript": "^3.5.0"
  },
  "nyc": {
    "nycrc-path": "./node_modules/@bentley/build-tools/.nycrc"
  }
}